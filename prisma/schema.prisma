generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
    id String @id @default(cuid())

    name String
    email String
    phone String
    password String

    created_at DateTime @default(now())
    updated_at DateTime @default(now())
}

model Prospect {
    id String @id @default(cuid())

    name String
    email String
    phone String
    message String

    created_at DateTime @default(now())
}